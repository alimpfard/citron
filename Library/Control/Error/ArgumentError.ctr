var ArgumentError is Object cnew: {
  message => ''.
}, on: '$' do: {:msg
  ^me cnew: {
    message => msg.
  }.
}, on: 'new' do: {
  ^me cnew: {
    message => ''.
  }.
}, on: 'type' do: {
  ^'ArgumentError'.
}, on: 'toString' do: {
  ^'ArgumentError: %s' % [my message].
}.

import export: ArgumentError as: 'ArgumentError'.
